{
  "name": "motion-plus-vue",
  "version": "1.2.1",
  "description": "Motion Plus Vue",
  "author": "",
  "license": "ISC",
  "keywords": [
    "ui",
    "vue"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "./LICENSE",
    "./dist"
  ],
  "typings": "./dist/index.d.ts",
  "peerDependencies": {
    "motion-v": ">=1.2.0",
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "motion-v": "^1.2.0",
    "vue": "^3.5.13"
  },
  "size-limit": [
    {
      "path": "dist/es/components/AnimateNumber/index.mjs",
      "limit": "3.75 kb",
      "ignore": [
        "vue",
        "motion-v"
      ]
    },
    {
      "path": "dist/es/components/Cursor/Cursor.vue.mjs",
      "limit": "3.1 kb",
      "ignore": [
        "vue",
        "motion-v"
      ]
    },
    {
      "path": "dist/es/components/Ticker/index.mjs",
      "limit": "2.8 kb",
      "ignore": [
        "vue",
        "motion-v"
      ]
    }
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "bumpp": "bumpp package.json",
    "pub:release": "pnpm publish --access public",
    "size": "size-limit"
  }
}